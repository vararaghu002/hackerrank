//https://www.hackerrank.com/contests/smart-interviews-basic/challenges/si-basic-compute-n/submissions/code/1354115206
#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
   int n,fact=1,i;
    cin>>n;
    for(i=1;i<=n;i++)
    {
        fact=fact*i;
    }
    cout<<fact;
    return 0;
}

